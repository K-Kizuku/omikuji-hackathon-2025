type User {
  id: ID!
  name: String!
}

type Python {
  id: ID!
  name: String!
  exp: Int!
  repository: String!
  user: User!
  stats: PythonStats!
}

type PythonStats {
  hp: Int!
  attack: Int!
  defense: Int!
  speed: Int!
  skills: [PythonSkill!]!
}

type PythonSkill {
  id: Int!
  name: String!
  description: String!
  pp: Int!
  attack: Int!
  hitRate: Int!
  minVersion: String!
  maxVersion: String!
}

type Query {
  me: User!
  python: Python!
}
